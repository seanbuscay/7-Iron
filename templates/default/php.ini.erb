; Can template this as desired.
session_cache_limiter = nocache
session.auto_start = 0
expose_php = 0
allow_url_fopen = 1
;Error reporting set On for dev
display_errors = 1
error_reporting = E_ALL
display_startup_errors = 1
memory_limit = 512M
upload_max_filesize = 500M
post_max_size = 500M
max_execution_time = 60
